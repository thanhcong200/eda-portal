{
  "kind": "collectionType",
  "collectionName": "winnovate_ideas",
  "info": {
    "singularName": "winnovate-idea",
    "pluralName": "winnovate-ideas",
    "displayName": "Winnovate Idea",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::winnovate-category.winnovate-category",
      "inversedBy": "ideas"
    },
    "topic": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::winnovate-topic.winnovate-topic",
      "inversedBy": "ideas"
    },
    "bu": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::winnovate-bu.winnovate-bu",
      "inversedBy": "ideas"
    },
    "name": {
      "type": "text"
    },
    "email": {
      "type": "text"
    },
    "score": {
      "type": "decimal"
    },
    "target_customer": {
      "type": "text"
    },
    "desc_target_customer": {
      "type": "text"
    },
    "problem_statement": {
      "type": "text"
    },
    "idea_owner": {
      "type": "text"
    },
    "pdf_url": {
      "type": "text"
    },
    "solution": {
      "type": "text"
    },
    "bookmarks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::winnovate-idea-bookmark.winnovate-idea-bookmark",
      "mappedBy": "idea"
    },
    "priority": {
      "type": "integer",
      "default": 2
    }
  }
}
